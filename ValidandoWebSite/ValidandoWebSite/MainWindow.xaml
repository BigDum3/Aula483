<Window x:Class="ValidandoWebSite.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ValidandoWebSite"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Loaded="Window_Loaded">
    <Grid>
        <TextBox Name="txtUrl" HorizontalAlignment="Left" Height="24" Margin="10,25,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="772" TextChanged="TxtUrl_TextChanged" Text="TextBox"/>
        <Button Click="Button_Click" Content="Não Clique Aqui" HorizontalAlignment="Left" Margin="10,54,0,0" VerticalAlignment="Top" Width="772" Height="72" RenderTransformOrigin="0.5,0.583"/>
        <TextBox Name="txtStatusReport" HorizontalAlignment="Left" Height="278" Margin="10,131,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="772" TextChanged="TextBox_TextChanged"/>
    </Grid>
</Window>
